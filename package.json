{
  "name": "goodrouter",
  "version": "0.0.0",
  "description": "a good router",
  "type": "module",
  "main": "out/main.js",
  "types": "out/main.d.ts",
  "files": [
    "out/*",
    "src/*"
  ],
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "clean": "rm -rf out",
    "test": "npm run lint && npm run spec-all",
    "lint": "eslint \"src/**/*.ts\"",
    "spec-all": "tape \"out/**/*.spec.js\"",
    "coverage": "c8 --reporter lcov npm run spec-all"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:goodrouter/goodrouter-ts.git"
  },
  "author": "Elmer Bulthuis",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/goodrouter/goodrouter-ts/-/issues"
  },
  "keywords": [
    "good",
    "router",
    "isomorphic",
    "universal"
  ],
  "homepage": "https://gitlab.com/goodrouter/goodrouter-ts",
  "devDependencies": {
    "@types/tape": "^4.13.0",
    "@types/tape-promise": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "c8": "^7.12.0",
    "eslint": "^8.27.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "tape": "^5.2.2",
    "tape-promise": "^4.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  }
}