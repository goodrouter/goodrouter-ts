{
  "name": "goodrouter",
  "description": "a good router",
  "type": "module",
  "main": "out/main.js",
  "types": "out/main.d.ts",
  "files": [
    "out/*",
    "src/*"
  ],
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "clean": "rm -rf out",
    "test": "npm run lint && npm run spec-all",
    "lint": "eslint \"src/**/*.ts\"",
    "spec-all": "tape-es \"out/**/*.spec.js\""
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:goodrouter/goodrouter-ts.git"
  },
  "author": "Elmer Bulthuis",
  "license": "WTFPL",
  "bugs": {
    "url": "https://gitlab.com/goodrouter/goodrouter-ts/-/issues"
  },
  "keywords": [
    "good",
    "router",
    "isomorphic",
    "universal"
  ],
  "homepage": "https://gitlab.com/goodrouter/goodrouter-ts",
  "devDependencies": {
    "@types/tape": "^4.13.0",
    "@types/tape-promise": "^4.0.1",
    "eslint-config-gameye": "^0.1.8",
    "tape": "^5.2.2",
    "tape-es": "^1.2.15",
    "tape-promise": "^4.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "tslib": "^2.2.0"
  }
}