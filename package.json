{
  "name": "goodrouter",
  "version": "0.0.0",
  "description": "a good router",
  "type": "module",
  "main": "out/main.js",
  "types": "out/main.d.ts",
  "files": [
    "out/*",
    "src/*"
  ],
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc",
    "clean": "rm -rf out",
    "test": "npm run spec-all",
    "lint": "eslint \"src/**/*.ts\"",
    "spec-all": "tape \"out/**/*.spec.js\"",
    "coverage": "c8 --reporter lcov --reporter cobertura npm test",
    "docs": "typedoc"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:luvdasun/goodrouter-ts.git"
  },
  "author": "LuvDaSun Software B.V.",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/goodrouter/goodrouter-ts/-/issues"
  },
  "keywords": [
    "good",
    "router",
    "isomorphic",
    "universal"
  ],
  "homepage": "https://ts.goodrouter.org",
  "devDependencies": {
    "@types/tape": "^4.13.2",
    "@types/tape-promise": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "c8": "^7.12.0",
    "eslint": "^8.33.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-security": "^1.7.0",
    "itertools": "^1.7.1",
    "tape": "^5.6.3",
    "tape-promise": "^4.0.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "tslib": "^2.5.0"
  }
}
